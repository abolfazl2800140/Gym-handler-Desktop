{
  "name": "gym-handler",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite --port 5173 --strictPort",
    "dev:electron": "concurrently -k \"cross-env VITE_DEV_SERVER_URL=http://localhost:5173 vite --port 5173 --strictPort\" \"wait-on http://localhost:5173 && nodemon --watch electron --ext cjs --exec electron ./electron/main.cjs\"",
    "build": "tsc -b && vite build",
    "build:watch": "tsc -b --watch & vite build --watch",
    "dev:electron:prod": "cross-env PROD_WATCH=true concurrently -k \"vite build --watch\" \"nodemon --watch electron --ext cjs --exec electron ./electron/main.cjs\"",
    "build:electron": "electron-builder --win --x64",
    "lint": "eslint .",
    "preview": "vite preview",
    "start": "electron ./electron/main.cjs"
  },
  "build": {
    "appId": "com.gym.handler",
    "directories": {
      "output": "release"
    },
    "files": [
      "dist/**",
      "electron/**",
      "package.json"
    ],
    "extraMetadata": {
      "main": "electron/main.cjs"
    },
    "win": {
      "target": [
        "nsis"
      ],
      "artifactName": "gym-handler-${version}-setup.${ext}"
    }
  },
  "dependencies": {
    "@fontsource/vazirmatn": "^5.2.8",
    "@heroicons/react": "^2.2.0",
    "@sentry/electron": "^5.1.1",
    "@xenova/transformers": "^2.17.2",
    "electron-log": "^5.4.3",
    "lowdb": "^7.0.1",
    "motion": "^12.23.22",
    "react": "^19.1.1",
    "react-dom": "^19.1.1",
    "recharts": "^3.2.1",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.36.0",
    "@tailwindcss/forms": "^0.5.10",
    "@tailwindcss/typography": "^0.5.19",
    "@types/node": "^24.6.0",
    "@types/react": "^19.1.16",
    "@types/react-dom": "^19.1.9",
    "@vitejs/plugin-react": "^5.0.4",
    "autoprefixer": "^10.4.21",
    "chokidar": "^4.0.3",
    "concurrently": "^9.0.1",
    "cross-env": "^10.1.0",
    "electron": "^33.0.2",
    "electron-builder": "^26.0.12",
    "eslint": "^9.36.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.22",
    "globals": "^16.4.0",
    "nodemon": "^3.1.10",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.13",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.45.0",
    "vite": "^5.4.8",
    "wait-on": "^8.0.2"
  }
}
